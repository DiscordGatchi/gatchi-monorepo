{
  "private": true,
  "name": "gatchi-monorepo",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "devDependencies": {
    "dotenv-cli": "^7.0.0",
    "prettier": "^2.8.3",
    "turbo": "^1.7.0",
    "typescript": "^4.9.4"
  },
  "scripts": {
    "ts": "dotenv -e ../../.env -c --",
    "build": "turbo build",
    "deploy:commands": "turbo deploy:commands",
    "start:kado": "yarn workspace kado start",
    "start:skia": "yarn workspace skia-api start",
    "start:anachi": "yarn workspace anachi start",
    "dev:skia": "yarn workspace skia-api dev",
    "start": "yarn workspaces foreach -pi run start"
  },
  "packageManager": "yarn@3.3.1",
  "dependencies": {
    "moment": "^2.29.4"
  }
}
